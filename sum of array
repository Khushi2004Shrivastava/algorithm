import java.io.*;
import java.math.*;
import java.security.*;
import java.text.*;
import java.util.*;
import java.util.concurrent.*;
import java.util.regex.*;



public class Solution {
    public static void main(String[] args){
        Scanner input=new Scanner(System.in);
        int n=input.nextInt();
        int a,sum=0;
        for(int i=0;i<n;i++){
        a=input.nextInt();
        sum=sum+a;}
        System.out.println(sum);
    }
}
